{
  "name": "aheenam/laravel-translatable",
  "description": "A Laravel Package to manage translations for models simply using a Trait",
  "license": "MIT",
  "authors": [
    {
      "name": "Rathes Sachchithananthan",
      "email": "rathes@aheenam.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php" : "^7.1",
    "illuminate/support": "~5.6.0|~5.7.0",
    "illuminate/database": "~5.6.0|~5.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "~3.6.0|~3.7.0",
    "mockery/mockery": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Aheenam\\Translatable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aheenam\\Translatable\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aheenam\\Translatable\\TranslatableServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test:windows": "vendor\\bin\\phpunit"
  }
}
